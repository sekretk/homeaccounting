  "scripts": {
    "start": "nx serve frontend",
    "start:backend": "nx serve backend",
    "start:all": "nx run-many -t serve -p frontend backend",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "test:watch": "nx run-many -t test --watch",
    "test:e2e": "nx run-many -t e2e",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "affected:e2e": "nx affected -t e2e",
    "graph": "nx graph",
    "clean": "nx reset",
    "prepare": "husky",
    "docker:build": "npm run docker:build:backend && npm run docker:build:frontend",
    "docker:build:backend": "docker build -f backend/Dockerfile -t homeaccounting-backend:dev .",
    "docker:build:frontend": "docker build -f frontend/Dockerfile -t homeaccounting-frontend:dev .",
    "docker:build:backend:prod": "docker build -f backend/Dockerfile -t homeaccounting-backend:latest .",
    "docker:build:frontend:prod": "docker build -f frontend/Dockerfile -t homeaccounting-frontend:latest .",
    "docker:tag:latest": "docker tag homeaccounting-backend:dev homeaccounting-backend:latest && docker tag homeaccounting-frontend:dev homeaccounting-frontend:latest",
    "k8s:start": "./scripts/k8s-dev.sh start",
    "k8s:stop": "./scripts/k8s-dev.sh stop",
    "k8s:status": "./scripts/k8s-dev.sh status",
    "k8s:logs": "./scripts/k8s-dev.sh logs",
    "k8s:restart": "./scripts/k8s-dev.sh restart",
    "k8s:build": "./scripts/k8s-dev.sh build",
    "colima:start": "./scripts/k8s-dev.sh colima start",
    "colima:stop": "./scripts/k8s-dev.sh colima stop",
    "colima:status": "./scripts/k8s-dev.sh colima status",
    "db:up": "./scripts/k8s-dev.sh start",
    "db:down": "./scripts/k8s-dev.sh stop",
    "db:logs": "./scripts/k8s-dev.sh logs postgres",
    "migration:create": "ts-node scripts/create-migration.ts",
    "migration:status": "echo 'Run the backend application to see migration status in logs'",
    "db:seed": "RUN_SEEDS=true node -e \"console.log('Set RUN_SEEDS=true and start the backend to run seeds')\"",
    "entities:generate": "./scripts/generate-entities.sh -db homeaccounting -h localhost -p 5432 -u postgres -w password",
    "entities:generate:custom": "./scripts/generate-entities.sh",
    "migrate": "ts-node --project tsconfig.base.json scripts/migrate.ts",
    "migrate:status": "npm run migrate status",
    "migrate:run": "npm run migrate run",
    "migrate:seeds": "npm run migrate seeds",
    "migrate:rollback": "npm run migrate rollback"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
